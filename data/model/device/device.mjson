    "associatedTaskTypes": {
        "prototype": "mod/core/meta/property-descriptor",
        "values": {
            "name": "associatedTaskTypes",
            "cardinality": -1,
            "inversePropertyName": "associatedToolTypes",
            "valueDescriptor": {"@": "TaskDescriptor"}
        }
