    "toolForDevices": {
        "prototype": "mod/core/meta/property-descriptor",
        "values": {
            "name": "toolForDevices",
            "cardinality": -1,
            "inversePropertyName": "tools",
            "valueDescriptor": {"@": "root"},
            "description": "The list of accessories actually used for by the device"
        }
