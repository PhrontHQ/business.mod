            "name": "DeviceConnection",
            "propertyDescriptors": [
                {"@": "nodeIdentifier"},
                {"@": "host"},
                {"@": "node"},
                {"@": "role"},
                {"@": "logicalNodeNumber"},
                {"@": "logicalNodeNumberRange"},
                {"@": "physicalInterface"},
                {"@": "logicalInterface"}

            ],
            "objectDescriptorModule": {
                "%": "./device-connection.mjson"
            },
            "exportName": "DeviceConnection",
            "module": {
                "%": "./device-connection.mjson"
            },
            "object":{"@": "DeviceConnection"},
            "parent":{"@": "Object"}
        }
    },
    "DeviceConnection": {
        "object": "./device-connection"
    },
    "Object": {
        "object": "mod/data/model/data-object.mjson"
    },
    "DeviceDescriptor": {
        "object": "./device.mjson"
    },
    "host": {
        "prototype": "mod/core/meta/property-descriptor",
        "values": {
            "name": "host",
            "cardinality": 1,
            "inversePropertyName": "hostDeviceConnections",
            "valueDescriptor": {"@": "DeviceDescriptor"},
            "description": "The host device"
        }
    },
    "node": {
        "prototype": "mod/core/meta/property-descriptor",
        "values": {
            "name": "node",
            "cardinality": 1,
            "inversePropertyName": "nodeDeviceConnections",
            "valueDescriptor": {"@": "DeviceDescriptor"},
            "description": "A device connected to the host"
        }
    },
    "RoleDescriptor": {
        "object": "mod/data/model/party/role.mjson"
    },
    "role": {
        "prototype": "mod/core/meta/property-descriptor",
        "values": {
            "name": "role",
            "cardinality": 1,
            "valueDescriptor": {"@": "RoleDescriptor"},
            "description": "The role this connection represents between the host and node devices"
        }
    },
