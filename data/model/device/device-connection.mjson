            "valueDescriptor": {"@": "RangeDescriptor"},
            "description": "The range within the logicalNodeNumber is expected to be. This will typically be used with node being a certain device that is a type"
        }
    },
    "nodeIdentifier": {
        "prototype": "mod/core/meta/property-descriptor",
        "values": {
            "name": "nodeIdentifier",
            "valueType": "string",
            "cardinality": 1,
