        }
    },
    "physicalInterface": {
        "prototype": "mod/core/meta/property-descriptor",
        "values": {
            "name": "physicalInterface",
            "valueType": "string",
            "cardinality": 1,
            "description": "Is it a slot (like PCIe), a port (like RJ-45), a cable? internal, external? May be worth a full object"
        }
    },
    "logicalInterface": {
        "prototype": "mod/core/meta/property-descriptor",
        "values": {
            "name": "logicalInterface",
            "valueType": "string",
            "cardinality": 1,
            "description": "What logical/digital construct is used on top of the physical. IVv4? IPv6? Thunderbolt 2,3, 4?"
        }
    }



