        "prototype": "mod/core/meta/property-descriptor",
        "values": {
            "name": "logicalNodeNumber",
            "valueType": "number",
            "cardinality": 1,
            "description": "A unique integer identifier assigned to a node within devices connected to a host, essentially representing its position within othe connected nodes, distinct from its physical hardware details"
        }
    },
    "RangeDescriptor": {
        "object": "mod/core/range.mjson"
    },
    "logicalNodeNumberRange": {
        "prototype": "mod/core/meta/property-descriptor",
